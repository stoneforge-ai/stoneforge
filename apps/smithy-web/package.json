{
  "name": "@stoneforge/smithy-web",
  "version": "1.0.3",
  "private": true,
  "type": "module",
  "description": "Stoneforge Smithy web UI for AI agent management",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:web": "vite build && mkdir -p ../../packages/smithy/web && rsync -a --delete dist/ ../../packages/smithy/web/",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "pretest": "test -d ../../packages/storage/dist || turbo run build --filter=@stoneforge/core --filter=@stoneforge/storage --filter=@stoneforge/quarry --force",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:ui": "playwright test --ui"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@stoneforge/ui": "workspace:*",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.65.0",
    "@tanstack/react-router": "^1.157.5",
    "@tanstack/react-virtual": "^3.13.18",
    "@tiptap/core": "^2.12.0",
    "@tiptap/extension-code-block-lowlight": "^2.12.0",
    "@tiptap/extension-placeholder": "^2.12.0",
    "@tiptap/extension-underline": "^2.12.0",
    "@tiptap/pm": "^2.12.0",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "@tiptap/suggestion": "^2.12.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "@xyflow/react": "^12.10.0",
    "cmdk": "^1.1.1",
    "emoji-picker-react": "^4.17.0",
    "idb": "^8.0.3",
    "jszip": "^3.10.1",
    "lowlight": "^3.3.0",
    "lucide-react": "^0.563.0",
    "marked": "^17.0.1",
    "monaco-editor": "^0.52.0",
    "react": "^19.1.0",
    "react-arborist": "^3.4.3",
    "react-dom": "^19.1.0",
    "react-resizable-panels": "^4.5.3",
    "sonner": "^2.0.7",
    "tippy.js": "^6.3.7",
    "turndown": "^7.2.0",
    "vscode-jsonrpc": "^8.2.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-ws-jsonrpc": "^3.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@stoneforge/core": "workspace:*",
    "@stoneforge/quarry": "workspace:*",
    "@stoneforge/storage": "workspace:*",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^19.0.10",
