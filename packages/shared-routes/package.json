{
  "name": "@stoneforge/shared-routes",
  "version": "1.10.0",
  "type": "module",
  "description": "Shared route factories for Stoneforge servers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoneforge-ai/stoneforge.git",
    "directory": "packages/shared-routes"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stoneforge/core": "workspace:*",
    "@stoneforge/storage": "workspace:*",
    "hono": "^4.7.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
